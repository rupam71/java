<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.FXMLDocumentController">
   <children>
      <Label layoutX="312.0" layoutY="177.0" text="Welcome To Red Line Service">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="362.0" layoutY="208.0" text=" Please log in to continue.">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="332.0" layoutY="266.0" text="User Id ">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="idTextField" layoutX="416.0" layoutY="260.0" onMouseClicked="#idTextFieldOnClick" />
      <Label layoutX="332.0" layoutY="301.0" text="Password">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="passwordTextField" layoutX="416.0" layoutY="299.0" onMouseClicked="#passwordTextFieldOnClick" />
      <Button fx:id="loginButton" layoutX="328.0" layoutY="424.0" mnemonicParsing="false" onAction="#loginButtonOnClick" prefHeight="26.0" prefWidth="67.0" text="Login" />
      <Label layoutX="332.0" layoutY="346.0" text="Group">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="loginComboBox" layoutX="417.0" layoutY="343.0" onAction="#loginComboBoxIsUpdate" prefWidth="150.0" />
      <Label fx:id="loginLabel" layoutX="417.0" layoutY="384.0" prefHeight="26.0" prefWidth="150.0" text="Admin">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label layoutX="332.0" layoutY="384.0" prefHeight="26.0" prefWidth="84.0" text="You Select ">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
